<html>
<head>
   <link href='css/materialize.css' rel='stylesheet' />
 <title>yo lean MVC</title>

</head>
<body>
<div class="container">
<div class="row">	
  <h2>yo quotes!</h2>
</div> 

  <% for (var i in quotes) { %>
  <div class="card">
  	<div class="card-content green">
  		<span class="white-text">
    <h3>quote: <%= quotes[i].quote %> </h3>
    <p>name: <%= quotes[i].name %> </p>
	</span>
    </div>
</div>
    <!-- <hr> -->
  <% } %>
</div>  
</body>
</html>